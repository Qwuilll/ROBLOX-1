TextLabel.Font = Enum.Font.Highway
TextLabel.Text = 'Meep City'
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 28

local Notification = Instance.new('Frame', Header)
