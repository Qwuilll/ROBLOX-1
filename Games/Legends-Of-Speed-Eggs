	CoreGui.LegendsOfSpeedEggs.Header.Notification.Purple.Size = UDim2.new(0, 35, 0, 35)
	CoreGui.LegendsOfSpeedEggs.Header.Notification.Red.Size = UDim2.new(0, 35, 0, 35)
	CoreGui.LegendsOfSpeedEggs.Header.Notification.Yellow.Size = UDim2.new(0, 40, 0, 35)
	CoreGui.LegendsOfSpeedEggs.Header.BackgroundColor3 = Color3.fromRGB(200, 200, 0)
	CoreGui.LegendsOfSpeedEggs.Header.Hide.BackgroundColor3 = Color3.fromRGB(200, 200, 0)
	CoreGui.LegendsOfSpeedEggs.Header.Notification.BackgroundColor3 = Color3.fromRGB(200, 200, 0)
	CoreGui.LegendsOfSpeedEggs.Header.Background.BackgroundColor3 = Color3.fromRGB(200, 200, 0)
	CoreGui.LegendsOfSpeedEggs.Header.Background.Egg.BackgroundColor3 = Color3.fromRGB(200, 200, 0)
	CoreGui.LegendsOfSpeedEggs.Header.Background.AutoHatchEggs.BackgroundColor3 = Color3.fromRGB(200, 200, 0)
end)
CoreGui.LegendsOfSpeedEggs.Header:TweenPosition(UDim2.new(0, 5, 0, 10), 'In', 'Back', 0.5)

while true do
	if AutoHatchEggsToggle == true then
		local Word = Egg.Text
		local Split = string.split(Word, ' ')
		for i, v in pairs(Split) do
			local First = v:sub(1, 1)
			local Rest = v:sub(2)
			Split[i] = First:upper().. Rest:lower()
		end
		local Result = table.concat(Split, ' ')
		game:GetService('ReplicatedStorage').rEvents.openCrystalRemote:InvokeServer("openCrystal", Result.. ' Crystal')
	end
	wait()
end
